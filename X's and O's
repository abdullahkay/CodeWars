public class XO {
  
  public static boolean getXO (String str) {
    
    int x = 0;
    int o = 0;
    
    for (int i = 0; i < str.length(); i++){
      char c = str.charAt(i);
      
      if (c == 'x' || c == 'X') {
        x++;
      }
      else if (c == 'o' || c =='O') {
        o++;
      }
    }
    return x == o;  
}
}
